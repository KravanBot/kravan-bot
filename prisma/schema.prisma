generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id String @id
  coins Int @default(0)
  bank Int @default(0)
  gems Int @default(0)
  daily Int @default(0)
  last_date DateTime?
  last_theft DateTime?
  jail DateTime?
  items Int[] @default([])
  minime Json?
}

model Jackpot {
  coins Int @id @default(0)
}
